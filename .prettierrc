{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": false,
	"trailingComma": "es5",
	"semi": true,
	"endOfLine": "auto",
	"arrowParens": "always",
	"bracketSpacing": true,
	"singleAttributePerLine": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"svelteSortOrder": "options-scripts-markup-styles",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {}
		}
	]
}
